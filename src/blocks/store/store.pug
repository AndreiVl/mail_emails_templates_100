mixin store(data)
	table.table-important(align="center" border="0" cellpadding="0" cellspacing="0")
		tr 
			td(align="left" style="color: #666666;" width="113")
				+img({
					link: '#',
					img_link: 'fashion-appstore-113-33-01.jpg',
					alt: 'apstore',
					width: 113,
					height: 33
				})

			+td-width(15)

			td(align="left" style="color: #666666;" width="113")
				+img({
					link: '#',
					img_link: 'fashion-googleplay-113-33-01.jpg',
					alt: 'googleplay',
					width: 113,
					height: 33
				})