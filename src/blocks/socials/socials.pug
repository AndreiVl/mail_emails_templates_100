mixin socials(data)
	if data.changeList
		- var changeList=data.changeList
	else
		-
			var changeList = [
				{
					type: 'icon',
					iconLink: '#',
					iconImg: 'templates-100-socila-46-inst.png',
					iconWidth: 46,
					iconHeight: 46
				},
				{
					type: 'margirn',
